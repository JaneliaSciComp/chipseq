nextflow_pipeline {

    name "Test Workflow main.nf"
    script "main.nf"
    tag "test_skip_consensus_peaks"
    tag "pipeline"

    test("Skip Consesus Peaks") {

        when {
            params {
                outdir               = "$outputDir"
                skip_consensus_peaks = true
            }
        }

        then {
            assertAll(
                { assert workflow.success }
            )
        }

    }

}
